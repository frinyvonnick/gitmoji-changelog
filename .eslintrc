{
  "extends": "airbnb-base/legacy",
  "env": {
    "jest": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "semi": ["error", "never"],
    "no-use-before-define": ["error", { "functions": false }],
    "comma-dangle": ["error", "always-multiline"],
    "no-console": "off",
    "no-param-reassign": "off"
  }
}
